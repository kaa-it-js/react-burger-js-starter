{
  "name": "vite_bundler",
  "homepage": "https://kaa-it-js.github.io/react-burger-js-starter/",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "commit": "cz",
    "check": "stylelint && eslint src/",
    "cypress:open": "cypress open",
    "cypress": "cypress run",
    "deploy": "gh-pages -d dist",
    "dev": "vite",
    "eslint": "eslint src/ --fix",
    "lint": "npm run stylelint && npm run eslint && npm run prettier",
    "predeploy": "npm run build",
    "prepare": "husky install",
    "prettier": "prettier \"./src/**/*.{js,jsx,ts,tsx,json,html,css,md}\" --write",
    "preview": "vite preview",
    "stylelint": "stylelint \"src/**/*.css\" --fix",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@babel/runtime": "^7.25.4",
    "@ya.praktikum/react-developer-burger-ui-components": "1.14.2",
    "core-js": "^3.38.1",
    "lodash-es": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/browser": "^3.1.2",
    "@vitest/ui": "^3.0.9",
    "commitizen": "^4.3.1",
    "cypress": "^14.2.0",
    "cypress-vite": "^1.6.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.2.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-unused-imports": "^4.1.4",
    "gh-pages": "^6.3.0",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.5.2",
    "prettier": "^3.5.3",
    "stylelint": "^16.19.1",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-order": "^7.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^6.3.5",
    "vite-plugin-checker": "^0.9.3",
    "vite-plugin-readable-classnames": "^1.5.0",
    "vite-plugin-sass-dts": "^1.3.31",
    "vitest": "^3.0.9"
  },
  "lint-staged": {
    "**/*.css": [
      "stylelint --fix",
      "prettier --write"
    ],
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{json,html,md}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
